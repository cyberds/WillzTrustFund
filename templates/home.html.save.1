<section>
  <div class="block remove-gap">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <div class="title">
            <span>get involved</span>
            <h2><i>OUR</i> Programs</h2>
          </div>

          <div class="creative-causes">
            <div class="row">
              {% for program in programs %}
              <div class="col-4 col-6">
                <div class="creative-cause">
                  <div class="cre-cause-img">
                    <img src="{{ program.image.url }}" alt="" />
                    <!-- <span><i>$653</i> Raised of $60,524 Goal</span>
                    <div class="skillbar">
                      <div class="progress"> <div class="progress-bar" style="width: 85%;"><span>85%</span></div> </div>
                    </div> -->
                    <a class="btn open-popup" href="#" title="">Donate Now</a>
                  </div>
                  <div class="cause-loc"><span>{{ program.program_name }}</span></div>
                  {% for key, value in program_desc.items %}
                    {% if key == program.id %}
                      <h3>{{ value }}</h3>
                    {% endif %}
                  {% endfor %}
                </div><!-- Creative Cause -->
              </div>
              {% endfor %}

            </div>
          </div><!-- Creative Causes -->
        </div>
      </div>
    </div>
  </div>
</section>

